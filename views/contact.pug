html
  head
    title Contact
    link(href='deathstar.ico', rel='icon', type='image/x-icon')
    link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css', integrity='sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T', crossorigin='anonymous')
    link(href='https://fonts.googleapis.com/css?family=Palanquin+Dark', rel='stylesheet')
    link(rel='stylesheet', href='styles.css')

    body#contact
      .container-fluid
        ul.nav
          li.nav-item
          a.nav-link(href='/') Home
          li.nav-item
          a.nav-link(href='people') Characters
          li.nav-item
          a.nav-link(href='planets') Planets
          li.nav-item
          a.nav-link(href='starships') Vehicles
          li.nav-item
          a.nav-link(href='contact') Contact

      img(src='SWLOGO1.png', alt='')

      .container
        h2 It's now or never
        h1 Join the Dark side!
        .container.mt-5
        form
          .row
            .col
              input#name.form-control(type='text', placeholder='Full Name')
            .col
              input#email.form-control(placeholder='Email', type='text')
        .container#button
          button#btAlert.btn.btn-success.btn-lg(type='submit') Submit
      img#b2b(src='24S3.gif', alt='')
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js')
  script(src='js/contact.js')
  script(src='https://unpkg.com/sweetalert/dist/sweetalert.min.js')
