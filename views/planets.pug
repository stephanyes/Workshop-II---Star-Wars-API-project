html
  head
    title Planets
    link(href='planets.ico', rel='icon', type='image/x-icon')
    link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css', integrity='sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T', crossorigin='anonymous')
    link(rel='stylesheet', href='styles.css')
    body#planets
  ul.nav
    li.nav-item
    a.nav-link(href='/') Home
    li.nav-item
    a.nav-link(href='people') Characters
    li.nav-item
    a.nav-link(href='planets') Planets
    li.nav-item
    a.nav-link(href='starships') Vehicles
    li.nav-item
    a.nav-link(href='contact') Contact
  .container
    h1.planets Planets
    h1.mobile Not Available for mobile yet.
    img#general(src='general.gif', alt='')
  .container.table-responsive.mt-5
    table#planets1.table.text-center
      thead
        tr
          th(scope='col') #
          th(scope='col') Planet
          th(scope='col') Weather
          th(scope='col') Gravity
          th.population(scope='col') Population
          th(scope='col') Action
          tbody#tableBody
  .containter#button
    input#button3.btn.btn-success.d-none(type='reset', value='Load more Planets')
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js')
  script(src='js/planets.js')
